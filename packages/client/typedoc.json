{
  "entryPoints": ["src/index.ts"],
  "out": "temp",
  "plugin": ["typedoc-plugin-markdown"],
  "readme": "none",
  "includeVersion": false,
  "excludePrivate": true,
  "excludeProtected": true,
  "excludeInternal": true,
  "excludeNotDocumented": true,
  "hideGenerator": true,
  "sort": ["source-order"],
  "kindSortOrder": [
    "Class",
    "Interface",
    "TypeAlias",
    "Enum",
    "Function",
    "Variable"
  ]
}